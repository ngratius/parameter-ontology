@dc:description "A description bundle for closed-world reasoning about a spacecraft"
description bundle <http://parameter.com/description/bundle#> as ^bundle {

    uses <http://purl.org/dc/elements/1.1/> as dc
    
    // The description bundle "uses" the vocabulary bundle world closure axioms
    uses <http://parameter.com/vocabulary/parameter-bundle#>

    // The description bundle "includes" the spacecraft description 
    includes <http://parameter.com/description/spacecraft#>
    
}